<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityDespliegueProyecto"
    android:background="@drawable/edificio">

    <TextView
        android:id="@+id/despliegueProyectoNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="83dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/despliegueProyectoDescripcion"/>

    <TextView
        android:id="@+id/despliegueProyectoDescripcion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="152dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/despliegueProyectoNombre"
        app:layout_constraintBottom_toTopOf="@+id/despliegueProyectoFecha1"/>

    <TextView
        android:id="@+id/despliegueProyectoFecha1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/despliegueProyectoDescripcion"
        app:layout_constraintBottom_toTopOf="@+id/despliegueProyectoFecha2" />

    <TextView
        android:id="@+id/despliegueProyectoFecha2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/despliegueProyectoFecha2"
        app:layout_constraintBottom_toTopOf="@+id/despliegueProyectoID" />

    <TextView
        android:id="@+id/despliegueProyectoID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/despliegueProyectoFecha2"
        app:layout_constraintBottom_toTopOf="@+id/botonRegresarAProyecto" />

    <Button
        android:id="@+id/botonRegresarAProyecto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:text="Regresar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/despliegueProyectoFecha2"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Button
        android:id="@+id/irATareasFiltradas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TAREAS"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/botonRegresarAProyecto"
        app:layout_constraintBottom_toTopOf="@id/irAReunionesFiltradas" />

    <Button
        android:id="@+id/irAReunionesFiltradas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REUNIONES"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/irATareasFiltradas"
        app:layout_constraintBottom_toBottomOf="parent" />

</android.support.constraint.ConstraintLayout>